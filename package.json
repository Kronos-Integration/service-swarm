    "docs": "documentation readme src/service-swarm.mjs --section=API",
    "lint": "documentation lint src/service-swarm.mjs"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^7.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "semantic-release": "^17.0.2"
  },
  "engines": {
    "node": ">=13.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-swarm.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-swarm/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-swarm#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-kronos-module.git"
    }
  }
